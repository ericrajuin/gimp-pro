# Windows 7 (Classic)
# --------------------

	# Default Styles
	# --------------------
		style "gimp-default-style" {
			bg[NORMAL] = "#d4d0c8" # background set to match "windows classic" theme in Windows 7
		}

	# Panel Drop area text
	# --------------------
		style "gimp-drop-text-style" = "gimp-default-style" {
			fg[NORMAL] = "#d4d0c8" # "you can drop docable dialogs here" label
		}
		widget_class "*GimpPanedBox.GtkLabel" style:highest "gimp-drop-text-style"

	# Empty image background
	# --------------------
		style "gimp-empty-image-style" = "gimp-default-style" {
		  bg[NORMAL] = "#d4d0c8" # canvas padding
		  fg[NORMAL] = "#d4d0c8" # Wilber
		}
		widget_class "*Canv*" style:highest "gimp-empty-image-style"

